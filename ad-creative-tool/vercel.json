{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 60
    },
    "app/api/**/*.tsx": {
      "maxDuration": 60
    }
  },
  "env": {
    "NEXT_PUBLIC_BASE_URL": "@next-public-base-url"
  }
}
